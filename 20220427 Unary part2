#include <stdio.h>
#include <iostream>
#include <math.h>
using namespace std;

int main(){
float a= sqrt(15);//menghitung akar kuadrat
float b= cbrt(27);//akar kubik
float c= pow(4.334,2.231);//menghitung pangkat
float d= sin(30);//menghitung sinus
float e= cos (30);//menghitung cosinus
float f= tan(30);//menghitung tangen

cout<<a<<endl;
cout<<b<<endl;
cout<<c<<endl;
cout<<d<<endl;
cout<<e<<endl;
cout<<f<<endl;

float g = round (3.12);
}
